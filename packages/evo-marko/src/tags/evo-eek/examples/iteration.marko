static const ratings = ["A", "B", "C", "D", "E", "F", "G"];
<const/max=ratings[0] />
<const/min=ratings[ratings.length - 1] />
<div>
    <for|rating| of=ratings>
        <const/label=`Energy Rating: ${rating}. Range ${max} - ${min}` />
        <evo-eek max=max min=min rating=rating aria-label=label />
    </for>
</div>
